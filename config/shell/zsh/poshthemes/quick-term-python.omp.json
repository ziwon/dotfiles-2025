{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "base": "#1e1e2e",
    "mantle": "#181825",
    "crust": "#11111b",
    "text": "#cdd6f4",
    "subtext1": "#bac2de",
    "subtext0": "#a6adc8",
    "overlay2": "#9399b2",
    "overlay1": "#7f849c",
    "overlay0": "#6c7086",
    "surface2": "#585b70",
    "surface1": "#45475a",
    "surface0": "#313244",
    "rosewater": "#f5e0dc",
    "flamingo": "#f2cdcd",
    "pink": "#f5c2e7",
    "mauve": "#cba6f7",
    "red": "#f38ba8",
    "maroon": "#eba0ac",
    "peach": "#fab387",
    "yellow": "#f9e2af",
    "green": "#a6e3a1",
    "teal": "#94e2d5",
    "sky": "#89dceb",
    "sapphire": "#74c7ec",
    "blue": "#89b4fa",
    "lavender": "#b4befe"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:mauve",
          "foreground": "p:base",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b2",
          "template": " {{ .Icon }} ",
          "type": "os"
        },
        {
          "background": "p:surface0",
          "foreground": "p:subtext0",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "p:sapphire",
          "foreground": "p:base",
          "properties": {
            "style": "agnoster_short",
            "max_depth": 3,
            "folder_icon": "\u2026",
            "folder_separator_icon": " <transparent>\ue0b1</> ",
            "mapped_locations": {
              "/cache": "/"
            }
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:peach",
          "foreground": "p:crust",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false,
            "fetch_version": true
          },
          "type": "python"
        },
        {
          "background": "p:green",
          "foreground": "p:crust",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:peach{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:maroon{{ end }}",
            "{{ if gt .Ahead 0 }}p:blue{{ end }}",
            "{{ if gt .Behind 0 }}p:blue{{ end }}"
          ],
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_stash_count": true,
            "fetch_status": true,
            "branch_icon": "\uf418 ",
            "branch_identical_icon": "\uf444",
            "branch_gone_icon": "\ueab8"
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <transparent>\ue0b1</> <p:base>\uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent>\ue0b1</> <p:base>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <transparent>\ue0b1</> <p:base>\ueb4b {{ .StashCount }}</>{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:surface1",
          "foreground": "p:overlay1",
          "properties": {
            "style": "austin",
            "always_enabled": true
          },
          "invert_powerline": true,
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "template": " \ueba2 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "p:lavender",
          "foreground": "p:base",
          "properties": {
            "time_format": "15:04:05"
          },
          "invert_powerline": true,
          "style": "diamond",
          "template": " \uf073 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:mauve",
          "style": "plain",
          "template": "\u2570\u2500 {{ if .Root }}#{{else}}${{end}}",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
